<%- include("header") -%>
  <div class="container">
    <table class="table table-borderless">
      <thead>
        <tr>
          <th scope="col">Name</th>
          <th scope="col">Unique ID</th>
          <th scope="col">Age</th>
          <th scope="col">Gender</th>
          <th scope="col">Date</th>
        </tr>
      </thead>
      <tbody>
        <% patientDataAll.forEach(function(item){ %>
          <tr>

            <td>
              <%= item.name %>
            </td>
            <td>
              <%= item.uniqueID %>
            </td>
            <td>
              <%= item.age %>
            </td>
            <td>
              <%= item.gender %>
            </td>
            <td>
              <%= item.date %>
            </td>
          </tr>
          <% }) %>
      </tbody>
    </table>
  </div>
  <%- include("footer") -%>