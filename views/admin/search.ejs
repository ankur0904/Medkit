<%- include("header") -%>
  <div>
    <table class="table table-borderless">
      <thead>
        <tr>
          <th scope="col">Name</th>
          <th scope="col">Unique ID</th>
          <th scope="col">Age</th>
          <th scope="col">Gender</th>
          <th scope="col">Date</th>
        </tr>
      </thead>
      <tbody>
        <% patientDataAll.forEach(function(item){ %>
          <tr>

            <td>
              <%= item.name %>
            </td>
            <td>
              <%= item.uniqueID %>
            </td>
            <td>
              <%= item.age %>
            </td>
            <td>
              <%= item.gender %>
            </td>
            <td>
              <%= item.date %>
            </td>
          </tr>
          <% }) %>
      </tbody>
    </table>
    </table>
  </div>
  <div class="foot" style="margin-top: 300px;">
    <section class="footer">
      <div class="social">
        <a href="#"><i class="fab fa-instagram"></i></a>
        <a href="#"><i class="fab fa-facebook"></i></a>
        <a href="#"><i class="fab fa-twitter"></i></a>
      </div>
      <ul class="list">
        <li>
          <a href="/">Home</a>
        </li>
        <li>
          <a href="/">Services</a>
        </li>
        <li>
          <a href="/">About</a>
        </li>
      </ul>
      <p class="copyright">Patel Dentals &copy; <%= new Date().getFullYear() %>
      </p>
      <p class="copyright">Designed By-YA</p>
    </section>
  </div>
  </body>
  </html>